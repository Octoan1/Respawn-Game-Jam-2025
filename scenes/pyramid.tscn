resource_name = "Pyramid"
script/source = "extends Node3D

@onready var cyl := $LightCylinder
@onready var door := $DoorMesh



func _ready() -> void:
	_pyramid_effect()

	
#func _process(delta: float) -> void:


#func _pyramid_door_open() -> void:

func _pyramid_effect() -> void:
	cyl.scale = Vector3(20,500,20)
	#cyl.position = lerp(Vector3(0,1000,0), Vector3(0,-1000,0), sin(delta))
	#print(sin(30 * delta))
	for i in 100:
		cyl.position = lerp(Vector3(0,1000,0), Vector3(0,-1000,0), i * .01)
		get_tree().create_timer(.1).timeout

	
"

