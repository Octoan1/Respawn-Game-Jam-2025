[sub_resource type="GDScript" id="GDScript_sw6ir"]
resource_name = "Pyramid"
script/source = "extends Node3D

@onready var cyl := $LightCylinder
@onready var door := $DoorMesh
@onready var gem_wall := $GemWall

var current_gem_index : int = 1
var open_door: bool = false

func _ready() -> void:
	#_pyramid_effect()
	GameManager.display_gem.connect(_on_display_gem)
	GameManager.all_gems_collected.connect(_on_all_gems_collected)

func _process(delta: float) -> void:
	if (open_door): 
		var speed = 2
		door.position.y -= speed * delta 
		gem_wall.position.y -= speed * delta
		if door.position.y < -60: open_door = false

func _on_display_gem(target: Gem):
	var color: Color = target.color
	var mat := StandardMaterial3D.new()
	mat.albedo_color = color
	
	var gem_mesh: MeshInstance3D = null
	
	match current_gem_index:
		1:
			gem_mesh = $\"GemWall/TopRow/Spawn1/GemMesh\"
		2:
			gem_mesh = $\"GemWall/TopRow/Spawn2/GemMesh\"
		3:
			gem_mesh = $\"GemWall/TopRow/Spawn3/GemMesh\"
		4:
			gem_mesh = $\"GemWall/TopRow/Spawn4/GemMesh\"
		5:
			gem_mesh = $\"GemWall/H Middle Room/Spawn1/GemMesh\"
		6: 
			gem_mesh = $\"GemWall/H Middle Room/Spawn2/GemMesh\"
		7: 
			gem_mesh = $\"GemWall/H Middle Room/Spawn3/GemMesh\"
		8: 
			gem_mesh = $\"GemWall/H Middle Room/Spawn4/GemMesh\"
		9: 
			gem_mesh = $\"GemWall/L Middle Room/Spawn1/GemMesh\"
		10: 
			gem_mesh = $\"GemWall/L Middle Room/Spawn2/GemMesh\"
		11:
			gem_mesh = $\"GemWall/L Middle Room/Spawn3/GemMesh\"
		12:
			gem_mesh = $\"GemWall/L Middle Room/Spawn4/GemMesh\"
		13: 
			gem_mesh = $\"GemWall/Bottom Row/Spawn1/GemMesh\"
		14:
			gem_mesh = $\"GemWall/Bottom Row/Spawn2/GemMesh\"
		15:
			gem_mesh = $\"GemWall/Bottom Row/Spawn3/GemMesh\"
		16: 
			gem_mesh = $\"GemWall/Bottom Row/Spawn4/GemMesh\"
	
	if gem_mesh != null:
		gem_mesh.material_override = mat
		gem_mesh.visible = true
	else:
		push_error(\"GemMesh not found for index: \" + str(current_gem_index))
		
	current_gem_index += 1


	#
##func _process(delta: float) -> void:
#
#
##func _pyramid_door_open() -> void:
#
#func _pyramid_effect() -> void:
	#cyl.scale = Vector3(20,500,20)
	##cyl.position = lerp(Vector3(0,1000,0), Vector3(0,-1000,0), sin(delta))
	##print(sin(30 * delta))
	#for i in 100:
		#cyl.position = lerp(Vector3(0,1000,0), Vector3(0,-1000,0), i * .01)
		#get_tree().create_timer(.1).timeout
		

func _on_all_gems_collected():
	open_door = true
	

	
"

